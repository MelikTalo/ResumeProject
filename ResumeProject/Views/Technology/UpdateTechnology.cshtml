@model ResumeProject.Models.TblTecnology
@{
    ViewBag.Title = "UpdateTechnology";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Teknoloji Güncelleme Sayfası</h2>
<form method="post">
    @Html.HiddenFor(x=>x.TecnologyID)
    <label>Teknoloji Adı</label>
    @Html.TextBoxFor(x => x.TecnologyTitle, new { @class = "form-control" })
    <br />
    <label>Teknoloji Değeri</label>
    @Html.TextBoxFor(x => x.TecnologyValue, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Verileri Kaydet</button>
</form>

