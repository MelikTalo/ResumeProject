@model List<ResumeProject.Models.TblSkill>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Yetenek Girişi</h2>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Yetenek Başlığı</th>
        <th>Yetenek Açıklaması</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.SkillID</td>
                    <td>@item.SkillTitle</td>
                    <td>@item.SkillDescription</td>
                    <td><a href="/Skill/DeleteSkill/@item.SkillID" class="btn btn-danger">Sil</a></td>
                    <td><a href="/Skill/UpdateSkill/@item.SkillID" class="btn btn-success">Güncelle</a></td>
                </tr>
            }
</table>
<a href="/Skill/AddSkill/" class="btn btn-primary">Yeni Yetenek Ekleme</a>




